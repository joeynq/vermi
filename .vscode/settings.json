{
	"cSpell.words": [
		"autowire",
		"awilix",
		"biomejs",
		"bunx",
		"commitlint",
		"datasource",
		"deburr",
		"deburred",
		"deepmerge",
		"jwks",
		"lefthook",
		"Mergeable",
		"mikro",
		"pino",
		"Proxify",
		"typebox"
	],
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"editor.codeActionsOnSave": {
		"quickfix.biome": "explicit",
		"source.organizeImports.biome": "explicit"
	},
	"editor.tabSize": 2,
	"exportall.config.relExclusion": [
		"/packages/utils/src/internal",
		"/packages/core/src/index.ts",
		"/packages/logger/src/logger",
		"/packages/cache/src/adapters",
		"/packages/logger/src/loggers"
	]
}
