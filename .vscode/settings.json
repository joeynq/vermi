{
	"cSpell.words": [
		"autowire",
		"awilix",
		"biomejs",
		"bunx",
		"commitlint",
		"datasource",
		"deburr",
		"deburred",
		"deepmerge",
		"dotfiles",
		"flushall",
		"Hasher",
		"jwks",
		"lefthook",
		"Mergeable",
		"mikro",
		"notifme",
		"pino",
		"Proxify",
		"typebox"
	],
	"editor.codeActionsOnSave": {
		"source.organizeImports.biome": "always"
	},
	"editor.tabSize": 2,
	"exportall.config.relExclusion": [
		"/packages/utils/src/internal",
		"/packages/core/src/index.ts",
		"/packages/logger/src/logger",
		"/packages/cache/src/adapters",
		"/packages/logger/src/loggers",
		"/packages/static/src/utils.ts"
	],
	"editor.defaultFormatter": "biomejs.biome",
	"biome.rename": true,
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	}
}
