		}
	},
	"type": "module",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@yab/core": "link:@yab/core"
	},
	"peerDependencies": {
		"@yab/core": "^0.0.0",
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"ioredis": "^5.4.1",
		"lru-cache": "^10.2.1"
	}
